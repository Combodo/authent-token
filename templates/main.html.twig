{% apply spaceless %}

{% UIMultiColumn Standard {} %}
	{% UIColumn Standard {} %}
		{% set index = 'user' %}
		{% set Infos = Params.user %}
		{% include 'section.html.twig' %}
	{% EndUIColumn %}
	{% if null != Params.contact %}
		{% UIColumn Standard {} %}
			{% set index = 'contact' %}
			{% set Infos = Params.contact %}
			{% include 'section.html.twig' %}
		{% EndUIColumn %}
	{% endif %}
{% EndUIMultiColumn %}

{% if null != Params.personaltoken %}
	{% UIColumn Standard {} %}
	{% set index = 'personaltoken' %}
	{% set Infos = Params.personaltoken %}
	{% include 'personaltoken.html.twig' %}
	{% EndUIColumn %}
{% endif %}

{% endapply %}
